#include <stdio.h>
#include "main.h"
#include <stdlib.h>
/**
 * main - add 2 or more positive numbers
 * @argc: number of arguments
 * @argv: arguments of the program
 * Return: always 0
 */
int main(int argc, char *argv[])
{
	int res;
	
	if (argc == 1)
	{
		printf("%d\n", 0);
	}
	else
	{
		for (l = 0; l < argc; l++)
		{
			res += aloi(argv[l]);
		}
		printf("%d\n", res);
	}

	return (0);
}
